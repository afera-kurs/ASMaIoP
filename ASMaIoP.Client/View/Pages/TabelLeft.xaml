<UserControl x:Class="ASMaIoP.Client.View.Pages.TabelLeft"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ASMaIoP.Client.View.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <DatePicker x:Name="DataPick"
                    SelectedDateChanged="DataPick_SelectedDateChanged"
                    Width="100"
                    Height="20"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Margin="20"></DatePicker>
    </Grid>
</UserControl>
